<%- include('./partials/start') %>

<div class="search-contain">
  <h1>
    Need a quick answer or can't find an offer that fits?
    <br />Give it a quick search.
  </h1>
  <form>
    <p>Search Term:</p>
    <input type="text" class="input-style title-input" />
    <p>Language:</p>
    <input type="text" class="input-style body-input" />
    <input
      class="button-style"
      id="search-submit"
      type="submit"
      value="Search"
    />
  </form>
</div>
<div class="search-results"></div>
<script src="main.js"></script>
<%- include('./partials/end') %>
